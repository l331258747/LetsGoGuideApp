<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="10dp">

    <LinearLayout
        android:id="@+id/ll_order_no"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:text="订单编号&#8195;&#8195;"
            android:textColor="@color/color_99" />

        <TextView
            android:id="@+id/tv_order_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:textSize="12sp"
            android:text="sdgag3q3q3"
            android:textColor="@color/color_99" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_order_create_time"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:text="创建时间&#8195;&#8195;"
            android:textColor="@color/color_99" />

        <TextView
            android:id="@+id/tv_order_create_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:layout_marginLeft="15dp"
            android:text="sdgag1234133q3q3"
            android:textColor="@color/color_99" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_order_pay_time"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="付款时间&#8195;&#8195;"
            android:textSize="12sp"
            android:textColor="@color/color_99" />

        <TextView
            android:id="@+id/tv_order_pay_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:layout_marginLeft="15dp"
            android:text="sdgag3q3q3"
            android:textColor="@color/color_99" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_order_pay_method"
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:text="付款方式&#8195;&#8195;"
            android:textColor="@color/color_99" />

        <TextView
            android:id="@+id/tv_order_pay_method"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:layout_marginLeft="15dp"
            android:text="sdgag3q3q3"
            android:textColor="@color/color_99" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_order_guide_time"
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:text="导游确认时间"
            android:textColor="@color/color_99" />

        <TextView
            android:id="@+id/tv_order_guide_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:textSize="12sp"
            android:text="sdgag3q3q3"
            android:textColor="@color/color_99" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_order_travel_start"
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:text="行程开始时间"
            android:textColor="@color/color_99" />

        <TextView
            android:id="@+id/tv_order_travel_start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:textSize="12sp"
            android:text="sdgag3q3q3"
            android:textColor="@color/color_99" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_order_travel_end"
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:text="行程结束时间"
            android:textColor="@color/color_99" />

        <TextView
            android:id="@+id/tv_order_travel_end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="sdgag3q3q3"
            android:textSize="12sp"
            android:textColor="@color/color_99" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_order_refund_apply"
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="申请退款时间"
            android:textSize="12sp"
            android:textColor="@color/color_99" />

        <TextView
            android:id="@+id/tv_order_refund_apply"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:textSize="12sp"
            android:text="sdgag3q3q3"
            android:textColor="@color/color_99" />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_order_refund_verify"
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="导游审核时间"
            android:textSize="12sp"
            android:textColor="@color/color_99" />

        <TextView
            android:id="@+id/tv_order_refund_verify"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="sdgag3q3q3"
            android:textSize="12sp"
            android:textColor="@color/color_99" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_order_refund_time"
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="退款时间&#8195;&#8195;"
            android:textSize="12sp"
            android:textColor="@color/color_99" />

        <TextView
            android:id="@+id/tv_order_refund_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="sdgag3q3q3"
            android:textSize="12sp"
            android:textColor="@color/color_99" />

    </LinearLayout>

</LinearLayout>
